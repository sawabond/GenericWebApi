{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "LifetimeMinutes": "#{Jwt.LifetimeMinutes}#",
    "Key": "#{Jwt.Key}#"
  },
  "DatabaseType": "#{DatabaseType}#",
  "ConnectionStrings": {
    "DefaultConnection": "#{DefaultConnection}#"
  },
  "MailSettingsOptions": {
    "DisplayName": "#{MailSettingsOptions.DisplayName}#",
    "From": "#{MailSettingsOptions.From}#",
    "Host": "#{MailSettingsOptions.Host}#",
    "Password": "#{MailSettingsOptions.Password}#",
    "Port": "#{MailSettingsOptions.Port}#",
    "UserName": "#{MailSettingsOptions.UserName}#",
    "UseSSL": "#{MailSettingsOptions.UseSSL}#",
    "UseStartTls": "#{MailSettingsOptions.UseStartTls}#"
  },
  "Authentication": {
    "Google": {
      "ClientId": "#{Google.OpenId.ClientId}#",
      "ClientSecret": "#{Google.OpenId.ClientSecret}#"
    }
  },
  "FeatureManagement": {
    "EmailVerification": "#{FeatureManagement.EmailVerification}#",
    "GoogleAuthentication": "#{FeatureManagement.GoogleAuthentication}#"
  }
}
